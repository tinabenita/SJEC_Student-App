<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/sjec"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SJECStudent"
        tools:targetApi="31">
        <activity
            android:name=".Developers"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".FullImageView"
            android:exported="false" />
        <activity
            android:name=".ebook.EbookActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.about.BranchModel"
            android:exported="true" />
        <activity
            android:name=".ui.gallery.GalleryAdapter"
            android:exported="true" />
    </application>

</manifest>